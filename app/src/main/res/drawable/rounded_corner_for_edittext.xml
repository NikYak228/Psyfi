<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="20dp" />
            <solid android:color="#55534E"/>
            <stroke
                android:width="0dp"
                android:color="#FFECE0BC" />
            <padding
                android:left="10dp"
                android:top="6dp"
                android:bottom="6dp"
                android:right="10dp"/>
        </shape>
    </item>
</layer-list>



